            "get": {
                "summary": "Get User Tags",
                "description": "Get all unique tags for a specific user.",
                "operationId": "get_user_tags_users__user_id__tags_get",
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "User Id"
                        }
                    },
                    {
                        "name": "include_archived",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "default": false,
